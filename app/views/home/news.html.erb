<h1>更新情報</h1>
<table class="news">
    <tbody>
        <% @news.each do |anew| %>
        <tr>
            <td><%= anew.created_at.strftime("%Y年%m月%d日") %></td>
            <td><%= anew.content %></td>
        </tr>
        <% end %>
    </tbody>
</table>
<div class="sponsored-link" style="margin-top: 50px; margin-bottom:400px">
    <h6>Sponsored Link</h6>
    <div class="ads-body">Sponsored Link</div>
</div>
